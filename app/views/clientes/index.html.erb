<h1>Listing clientes</h1>

<table>
  <tr>
    <th>Matricula</th>
    <th>Nome</th>
    <th>Sobrenome</th>
    <th>Lotacao</th>
    <th>Localidade</th>
    <th>Empresa</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @clientes.each do |cliente| %>
  <tr>
    <td><%= cliente.matricula %></td>
    <td><%= cliente.nome %></td>
    <td><%= cliente.sobrenome %></td>
    <td><%= cliente.lotacao %></td>
    <td><%= cliente.localidade %></td>
    <td><%= cliente.empresa %></td>
    <td><%= link_to 'Show', cliente %></td>
    <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
    <td><%= link_to 'Excluir', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Cliente', new_cliente_path %>
